############ STACK ################
class Stack:
  def __init__(self):
    self.myList=list()
    self.n=0
  def push(self,value):
    self.myList.append(value)
  def pop(self):
    if len(self.myList)==0:
      raise Exception('Cannot delete element from the stack')
    else:
      del self.myList[-1]
    return self.myList
  def top(self):
    return self.myList[len(self.myList)-1]
  def __repr__(self):
    return '__Stack Object__:'+''.join(str(self.myList))
  def __len__(self):
    return len(self.myList)
  def empty(self):
    if (len(self.myList))==0:
      return True
    return False

s=Stack()
s.push(5)
s.push(8)
s.push(14)
s.push(7)
s.push(3)
while(not s.empty()):
  print(s.top())
  s.pop()
  
############### Example of class ##############
class Student:
  def __init__(self, name, age, Id):
    self.name=name
    self.age=age
    self.Id=Id
  def __repr__(self):
    return f'The student name is: '+self.name+' its age is: '+str(self.age)+' and its id is: '+str(self.Id)
  def __le__(self,obj):
    return self.age<=obj.age
  def __lt__(self,obj):
    return self.age<obj.age
  def __ne__(self,obj):
    return self.age!=obj.age
  def __eq__(self,obj):
    return self.age==obj.age
  def __gt__(self,obj):
    return self.age>obj.age
  def __ge__(self,obj):
    return self.age>=obj.age

student=Student('Paul',15,1482)
stu=Student('Xavier',19,4565)
print(stu>student)
